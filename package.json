{
  "name": "gulp-starter",
  "version": "0.1.1",
  "description": "Gulp starter with common tasks and scenarios",
  "repository": {
    "type": "git",
    "url": "git://github.com/greypants/gulp-starter.git"
  },
  "browser": {
    "plugin": "./src/javascript/vendor/jquery-plugin.js"
  },
  "browserify": {
    "transform": [
      "browserify-shim",
      "coffeeify",
      "html2js-browserify",
      "jade2js-browserify"
    ]
  },
  "browserify-shim": {
    "plugin": {
      "exports": "plugin",
      "depends": [
        "jquery:$"
      ]
    }
  },
  "devDependencies": {
    "browser-sync": "~0.8.2",
    "browserify-shim": "~3.4.1",
    "browserify": "~3.36.0",
    "chai": "~1.9.1",
    "coffee-script": "~1.7.1",
    "coffeeify": "~0.6.0",
    "connect": "~2.14.3",
    "gulp-changed": "~0.3.0",
    "gulp-compass": "~1.1.8",
    "gulp-imagemin": "~0.1.5",
    "gulp-notify": "~1.2.4",
    "gulp-open": "~0.2.8",
    "gulp": "~3.8.0",
    "hbsfy": "~1.3.2",
    "vinyl-source-stream": "~0.1.1",
    "watchify": "~0.10.1",
    "gulp-util": "~2.2.14",
    "pretty-hrtime": "~0.2.1",
    "html2js-browserify": "0.0.2",
    "gulp-mocha": "~0.4.1",
    "html2js-browserify": "git://github.com/ArnaudRinquin/html2js-browserify#feature_preprocessors",
    "jade2js-browserify": "0.0.3",
    "mocha": "~1.19.0",
    "chai": "~1.9.1",
    "coffee-script": "~1.7.1"
  },
  "dependencies": {
    "jquery": "~2.1.0",
    "angular": "~1.2.16",
    "angular-timer": "git://github.com/ArnaudRinquin/angular-timer#feature_commonjs",
    "angular-ui-router": "~0.2.10"
  }
}
