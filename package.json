{
  "name": "gulp-starter",
  "version": "0.1.1",
  "description": "Gulp starter with common tasks and scenarios",
  "repository": {
    "type": "git",
    "url": "git://github.com/greypants/gulp-starter.git"
  },
  "browser": {
    "jquery": "./bower_components/jquery/dist/jquery.js",
    "angular": "./bower_components/angular/angular.min.js",
    "angular-cookies": "./bower_components/angular-cookies/angular-cookies.min.js",
    "angular-ui-router": "./bower_components/angular-ui-router/release/angular-ui-router.min.js",
    "angular-local-storage": "./bower_components/Angular-localStorage/src/angularLocalStorage.js"
  },
  "scripts": {
    "postinstall": "bower install",
    "test": "gulp testOnce",
    "start": "node server"
  },
  "browserify": {
    "transform": [
      "browserify-shim",
      "coffeeify",
      "html2js-browserify",
      "jade2js-browserify"
    ]
  },
  "browserify-shim": {
    "jquery": {
      "exports": "$"
    },
    "angular": {
      "exports": "angular",
      "depends": "jquery"
    },
    "angular-ui-routeur": {
      "depends": "angular"
    },
    "angular-cookies": {
      "depends": "angular"
    },
    "angular-local-storage": {
      "depends": [
        "angular",
        "angular-cookies"
      ]
    }
  },
  "devDependencies": {
    "bower": "~1.3.8",
    "browser-sync": "~0.8.2",
    "browserify": "~3.36.0",
    "browserify-shim": "~3.4.1",
    "chai": "~1.9.1",
    "coffee-script": "~1.7.1",
    "coffeeify": "~0.6.0",
    "express": "~4.7.2",
    "gulp": "~3.8.0",
    "gulp-changed": "~0.3.0",
    "gulp-compass": "~1.1.8",
    "gulp-imagemin": "~0.1.5",
    "gulp-karma": "0.0.4",
    "gulp-notify": "~1.2.4",
    "gulp-open": "~0.2.8",
    "gulp-util": "~2.2.14",
    "html2js-browserify": "git://github.com/ArnaudRinquin/html2js-browserify#feature_preprocessors",
    "jade2js-browserify": "0.0.3",
    "karma": "~0.12.16",
    "karma-browserify": "^0.2.1",
    "karma-chrome-launcher": "^0.1.4",
    "karma-coffee-preprocessor": "^0.2.1",
    "karma-jasmine": "^0.2.2",
    "karma-osx-reporter": "~0.1.0",
    "karma-phantomjs-launcher": "~0.1.4",
    "pretty-hrtime": "~0.2.1",
    "vinyl-source-stream": "~0.1.1",
    "watchify": "~0.10.1",
    "karma-mocha-reporter": "~0.2.8"
  }
}
